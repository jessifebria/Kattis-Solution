
import java.util.ArrayList;
import java.util.Scanner;

public class Kattistry {

    public static void main(String[] args) {
        Scanner scan=new Scanner (System.in);
        
        ArrayList<Integer> d = new ArrayList<>();
        int c=0;
        for (int i = 0; i < 10; i++) {
            int b=scan.nextInt();
            int a= b%42;
            if (!(d.contains(a))){
                d.add(a);
            }
        }
        System.out.println(d.size());
        }
    }
