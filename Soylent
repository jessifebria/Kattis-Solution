
import java.util.Scanner;
public class Kattistry {
    
 public static void main(String[] args)  {
   Scanner scan= new Scanner (System.in);
   int a=scan.nextInt();
    
         
     
     for (int i = 0; i < a; i++) {
      int b=scan.nextInt();
         
      System.out.println((int)Math.ceil(b/400.0));
     
     }
   }
 }
