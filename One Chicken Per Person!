#include <stdio.h>
#include <stdlib.h>

int main()
{
    int a,b;
    scanf("%d",&a);
    scanf("%d",&b);

    if  (a<=b){
            if((b-a)<=1){
                 printf("Dr. Chaz will have %d piece of chicken left over!\n",(b-a));

            }
            else{
        printf("Dr. Chaz will have %d pieces of chicken left over!\n",(b-a));
    }}
    else {
            if((a-b)<=1){
        printf("Dr. Chaz needs %d more piece of chicken!\n",(a-b));

            }else{
        printf("Dr. Chaz needs %d more pieces of chicken!\n",(a-b));
            }
    }



return 0;
}
